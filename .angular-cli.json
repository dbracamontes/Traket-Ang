{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "base-web"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico",
        "../src/assets/",
        "../src/assets/images"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "base-web",
      "styles": [
        
        "../src/assets/plugins/bootstrap/css/bootstrap.min.css",
        "../src/assets/plugins/bootstrap-extension/css/bootstrap-extension.css",
        "../src/assets/css/animate.css",
        "../src/assets/css/style.css",
        "../src/assets/css/colors/default-dark.css"
      ],
      "scripts": [
        "../src/assets/plugins/jquery/jquery.min.js",
        "../src/assets/plugins/bootstrap/js/tether.min.js",
        "../src/assets/plugins/bootstrap/js/bootstrap.min.js",
        "../src/assets/plugins/bootstrap-extension/js/bootstrap-extension.min.js",
        "../src/assets/js/jquery.slimscroll.js",
        "../src/assets/js/sidebarmenu.js",
        "../src/assets/plugins/sparkline/jquery.sparkline.min.js",
        "../src/assets/js/custom.js",
        "../src/assets/js/pace.min.js",
        "../src/assets/plugins/styleswitcher/jQuery.style.switcher.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}